-- Insert rows from journal_entries to 1_journal_entries where wallet_division = 1
INSERT INTO 1_journal_entries (amount, balance, context_id, context_id_type, date, description, 
                                first_party_id, id, reason, ref_type, second_party_id, 
                                wallet_division, transaction_type)
SELECT amount, balance, context_id, context_id_type, date, description, 
       first_party_id, id, reason, ref_type, second_party_id, 
       wallet_division, transaction_type
FROM journal_entries
WHERE wallet_division = 1;

